<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="704.0" prefWidth="540.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.PLCController">
   <center>
      <VBox prefHeight="750.0" prefWidth="540.0" BorderPane.alignment="CENTER">
         <children>
            <HBox alignment="CENTER" prefHeight="150.0" prefWidth="200.0">
               <children>
                  <Label text="年" />
                  <ComboBox fx:id="fx_combo_year" prefWidth="150.0">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </ComboBox>
                  <Label text="月">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <ComboBox fx:id="fx_combo_month" prefWidth="150.0">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </ComboBox>
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </HBox>
            <Label text="賞与手当" />
            <TextField fx:id="fx_text_bonus_allowance">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin></TextField>
            <Label text="賞与引当金繰入" />
            <TextField fx:id="fx_text_provision_of_bonus_allowance">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin></TextField>
            <Label text="通勤交通費" />
            <TextField fx:id="fx_text_commuter_traffic_expenses">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </TextField>
            <Label text="通信費" />
            <TextField fx:id="fx_text_communication_cost">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </TextField>
            <Label text="バックログ" />
            <TextField fx:id="fx_text_back_log">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin></TextField>
            <Label text="家賃地代" />
            <TextField fx:id="fx_text_lease_fee">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin></TextField>
            <Label text="リース料" />
            <TextField fx:id="fx_text_rent_fee">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin></TextField>
            <Label text="修繕費" />
            <TextField fx:id="fx_text_repair_costs">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin></TextField>
            <Label text="保守管理費" />
            <TextField fx:id="fx_text_maintenance_fee">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </TextField>
            <Label text="保険料" />
            <TextField fx:id="fx_text_insurance_fee">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </TextField>
            <Label text="求人費" />
            <TextField fx:id="fx_text_recruiting_expenses">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </TextField>
            <Label text="諸会費" />
            <TextField fx:id="fx_text_fees">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </TextField>
            <Label text="新聞図書費" />
            <TextField fx:id="fx_text_newspaper_book_expenses">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </TextField>
            <Label text="雑費" />
            <TextField fx:id="fx_text_miscellaneous_expenses">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin>
            </TextField>
         </children>
      </VBox>
   </center>
   <top>
      <Label text="月次経費登録" BorderPane.alignment="CENTER">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </top>
   <bottom>
      <HBox prefHeight="51.0" prefWidth="540.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#OnAddButtonClick" text="登録">
               <HBox.margin>
                  <Insets />
               </HBox.margin></Button>
            <Button mnemonicParsing="false" text="中止" />
         </children>
      </HBox>
   </bottom>
</BorderPane>
